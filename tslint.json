{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "semi": false,
        "singleQuote": true
      }
    ],
    "no-console": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false
  }
}
